{
  "manifest_version": 2,
  "name": "Website Highlighter",
  "version": "1.0",
  "description": "Highlights different phrases on different websites",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
      "matches": [ "<all_urls>" ],
      "js": [ "jquery.js", "jquery.highlight-3.js", "selection.js" ]
   } ],
  "permissions": [
  "tabs", "http://*/*", "https://*/*"
  ]
}
