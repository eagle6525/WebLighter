{
  "manifest_version": 2,
  "name": "Website Highlighter",
  "version": "1.0",
  "description": "Highlights different phrases on different websites",
  "content_scripts": [ {
      "matches": [ "<all_urls>" ],
      "js": [ "jquery.js", "jquery.highlight-3.js", "selection.js" ]
   } ],
  "permissions": [
  "tabs"
  ],
  "icons" : {
    "48" : "sandwich-48.png",
    "128" : "sandwich-128.png"
  }
}
